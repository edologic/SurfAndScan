{
  "name": "surfandscan",
  "version": "1.2.1",
  "description": "Framework to use scan devices in your browser",
  "main": "index.js",
  "scripts": {
    "test": "standard && karma start",
    "dev": "webpack --mode development --progress --hide-modules --devtool source-map",
    "watch": "webpack --mode development --watch --progress --hide-modules",
    "production": "webpack --mode production --progress --hide-modules",
    "start": "webpack-dev-server --public 'http://localhost:8080'",
    "standard:fix": "standard --fix"
  },
  "standard": {
    "ignore": [
      "/dist/sas.js"
    ],
    "env": {
      "browser": true,
      "jasmine": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edologic/SurfAndScan.git"
  },
  "keywords": [
    "scan",
    "scanner",
    "device",
    "browser",
    "print",
    "printer",
    "js"
  ],
  "author": "Matthias Ebel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edologic/SurfAndScan/issues"
  },
  "homepage": "https://github.com/edologic/SurfAndScan#readme",
  "dependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "ansi-html-community": "0.0.8",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "babel-loader": "8.1.0",
    "coveralls": "3.1.0",
    "css-loader": "4.2.2",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine-core": "3.6.0",
    "karma": "5.2.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.8",
    "karma-webpack": "4.0.2",
    "mini-css-extract-plugin": "0.11.0",
    "node-sass": "4.14.1",
    "nth-check": ">=2.0.1",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "sass-loader": "10.0.2",
    "standard": "14.3.4",
    "terser-webpack-plugin": "4.1.0",
    "webpack": "4.44.1",
    "trim-newlines": ">=3.0.1",
    "webpack-cli": "3.3.12",
    "tar": ">=4.4.18",
    "webpack-dev-server": "3.11.0",
    "ua-parser-js": ">=0.7.24"
  }
}
